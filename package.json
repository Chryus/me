{
  "name": "portfolio-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://chryus.github.io/react_portfolio/",
  "dependencies": {
    "gh-pages": "^2.2.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-scripts": "2.1.5",
    "travis": "^0.1.1",
    "yarn": "^1.13.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "clean": "rm -rf build/*",
    "predeploy": "npm run build",
    "release": "npm run clean && npm run predeploy",
    "deploy": "cd build && git add --all && git commit -m \"Release at $(date)\" && git push origin gh-pages",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
